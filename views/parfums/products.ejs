                                    <div class="cart-list">
                                        <% if (cart && cart.products.length> 0) { %>
                                            <% cart.products.forEach(item=> { %>
                                                <div class="product-widget">
                                                    <div class="product-img">
                                                        <img src="<%= item.product.image %>" alt="<%= item.product.name %>">
