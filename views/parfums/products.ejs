                                        <small>
                                            <%= wishlist ? wishlist.products.length : 0 %> Item(s) selected
                                        </small>
                                        <h5>SUBTOTAL: <%= wishlist ? wishlist.products.reduce((total, item)=> total +
                                                (item.product.price * item.quantity),
                                                0) : 0 %>DH</h5>
                                    </div>
                                    <div class="cart-btns">
                                        <a href="#">View Cart</a>
                                        <a href="/parfum/checkout">Checkout <i class="fa fa-arrow-circle-right"></i></a>
                                    </div>
                                </div>
                            </div>
